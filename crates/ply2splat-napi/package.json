{
  "name": "ply2splat-native",
  "version": "0.4.4",
  "description": "Native Node.js bindings for ply2splat",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bastikohn/ply2splat"
  },
  "license": "MIT",
  "bin": {
    "ply2splat": "./cli.js"
  },
  "scripts": {
    "build": "pnpm run build:debug",
    "build:debug": "napi build --platform",
    "build:release": "napi build --platform --release",
    "prepublishOnly": "napi prepublish -t npm",
    "artifacts": "napi artifacts",
    "version": "napi version"
  },
  "napi": {
    "name": "ply2splat-native"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  },
  "engines": {
    "node": ">= 10"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
